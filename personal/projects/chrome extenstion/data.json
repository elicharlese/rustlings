{
    "name": "Hello World Extension",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Displays a 'Hello, World!' message when clicked.",
    "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
    },
    "browser_action": {
    "default_icon": "icon32.png",
    "default_title": "Hello, World!"
    },
    "permissions": [
    "activeTab"
    ]
}
